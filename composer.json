{
    "name": "lafourchette/switch-user-stateless-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to use impersonating on Symfony API",
    "keywords": [
        "REST",
        "API",
        "Impersonating"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Chalamon",
            "email": "vincentchalamon@gmail.com",
            "homepage": "http://blog.vincent-chalamon.fr"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/http-kernel": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.3|^3.0",
        "symfony/http-foundation": "^2.3|^3.0",
        "symfony/security-core": "^2.3|^3.0",
        "symfony/config": "^2.3|^3.0",
        "symfony/security-bundle": "^2.3|^3.0",
        "symfony/security-http": "^2.3|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<5.0",
        "doctrine/orm": "^2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "^1.2",
        "behat/behat": "^3.0",
        "behat/symfony2-extension": "^2.0",
        "symfony/serializer": "^2.3|^3.0",
        "symfony/browser-kit": "^2.3|^3.0"
    },
    "suggest": {
        "psr/log": "To log user requests",
        "symfony/event-dispatcher": "To dispatch a switch user event"
    },
    "autoload": {
        "psr-4": {
            "LaFourchette\\SwitchUserStatelessBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaFourchette\\SwitchUserStatelessBundle\\Tests\\": "features/app"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
